{
    "name": "@cyb3r-jak3/workers-common",
    "version": "5.5.1",
    "description": "Common typescript code that I use between my cloudflare workers projects.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "vitest run --coverage",
        "build:new": "npm run clean && npm run build",
        "build": "tsc",
        "clean": "tsc --build --clean",
        "format": "prettier --write *.json tests/**/*.ts src/**/*.ts",
        "lint": "eslint --max-warnings=0 src/**/*.ts && prettier --check *.json tests/**/*.ts src/**/*.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Cyb3r-Jak3/common-typescript.git"
    },
    "author": "Cyb3r-Jak3",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Cyb3r-Jak3/common-typescript/issues"
    },
    "keywords": [
        "cyb3r-jak3",
        "shared",
        "workers",
        "cloudflare",
        "cloudflare-workers"
    ],
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "homepage": "https://github.com/Cyb3r-Jak3/common-typescript",
    "devDependencies": {
        "@cloudflare/vitest-pool-workers": "^0.8.34",
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.15.0",
        "@eslint/json": "^0.13.0",
        "@vitest/coverage-istanbul": "^3.0.9",
        "eslint": "^9.28.0",
        "eslint-config-prettier": "^10.1.5",
        "globals": "^16.2.0",
        "prettier": "^3.0.0",
        "typescript": "^5.1.6",
        "typescript-eslint": "^8.33.1",
        "vitest": "3.2.4"
    }
}
